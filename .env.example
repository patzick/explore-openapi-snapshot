# Environment variables for E2E testing
# Copy this file to .env and fill in your actual values

# API endpoint URL for testing (use local dev server or production)
API_URL=http://localhost:8779/v1/snapshot
# API_URL=https://editor-api.explore-openapi.dev/public/v1/snapshot

# Authentication token for the explore-openapi.dev API
API_AUTH_TOKEN=your_api_token_here

# Test project name or ID
TEST_PROJECT=test-project-e2e

# Repo to test
TEST_REPO=patzick/explore-openapi-snapshot/

# Test snapshot name (will be used for E2E test snapshots)
TEST_SNAPSHOT_NAME=e2e-test-snapshot
